{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "Todo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "task": {
      "type": "string"
    },
    "condition": {
      "type": "enumeration",
      "enum": [
        "open",
        "done",
        "failed"
      ]
    },
    "check_in": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::check-in.check-in",
      "inversedBy": "values"
    }
  }
}
